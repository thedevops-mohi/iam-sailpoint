        "name": "LCSPrehireCheck"
    },
    "object": {
        "id": "953fa2f1-a687-4f4c-a5d1-4e9a8523fe63",
        "name": "LCSPrehireCheck",
        "type": "static",
        "attributes": {
            "isFutureStartDate": {
                "type": "firstValid",
                "attributes": {
                    "values": [
                        {
                            "type": "dateCompare",
                            "attributes": {
                                "firstDate": {
                                    "type": "dateFormat",
                                    "attributes": {
                                        "input": {
                                            "type": "reference",
                                            "attributes": {
                                                "id": "StartDate"
                                            }
                                        },
                                        "inputFormat": "yyyy-MM-dd",
                                        "outputFormat": "ISO8601"
                                    }
                                },
                                "secondDate": "now",
                                "operator": "gt",
                                "positiveCondition": "true",
                                "negativeCondition": "false"
                            }
                        },
                        "false"
                    ]
                }
            },
            "isEndDateGteToday": {
                "type": "firstValid",
                "attributes": {
                    "values": [
                        {
                            "type": "dateCompare",
                            "attributes": {
                                "firstDate": {
                                    "type": "dateFormat",
                                    "attributes": {
                                        "input": {
                                            "type": "reference",
                                            "attributes": {
                                                "id": "EndDate"
                                            }
                                        },
                                        "inputFormat": "yyyy-MM-dd",
                                        "outputFormat": "ISO8601"
                                    }
                                },
                                "secondDate": "now",
                                "operator": "gte",
                                "positiveCondition": "true",
                                "negativeCondition": "false"
                            }
                        },
                        "false"
                    ]
                }
            },
            "endDate": {
                "type": "reference",
                "attributes": {
                    "id": "EndDate"
                }
            },
            "value": "#if($isFutureStartDate == 'true' && $endDate == 'None')true#elseif($isFutureStartDate == 'true' && $isEndDateGteToday)true#{else}false#end"
        },
        "internal": false
    }
