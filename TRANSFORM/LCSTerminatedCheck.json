{
    "version": 1,
    "var_self": {
        "type": "TRANSFORM",
        "id": "8e7fa6c7-a21a-4a35-b30f-e975291dd47e",
        "name": "LCSTerminatedCheck"
    },
    "object": {
        "id": "8e7fa6c7-a21a-4a35-b30f-e975291dd47e",
        "name": "LCSTerminatedCheck",
        "type": "static",
        "attributes": {
            "userType": {
                "type": "reference",
                "attributes": {
                    "id": "UserType"
                }
            },
            "empStat1_T": {
                "type": "firstValid",
                "attributes": {
                    "values": [
                        {
                            "type": "accountAttribute",
                            "attributes": {
                                "sourceName": "Employees SAP ECC",
                                "attributeName": "STAT1_T"
                            }
                        },
                        "None"
                    ]
                }
            },
            "isEndDateWithinPlus21Days": {
                "type": "firstValid",
                "attributes": {
                    "values": [
                        {
                            "type": "dateCompare",
                            "attributes": {
                                "firstDate": {
                                    "type": "dateMath",
                                    "attributes": {
                                        "expression": "-21d/d",
                                        "roundUp": true,
                                        "input": {
                                            "type": "dateFormat",
                                            "attributes": {
                                                "input": {
                                                    "type": "reference",
                                                    "attributes": {
                                                        "id": "EndDate"
                                                    }
                                                },
                                                "inputFormat": "yyyy-MM-dd",
                                                "outputFormat": "ISO8601"
                                            }
                                        }
                                    }
                                },
                                "secondDate": "now",
                                "operator": "lte",
                                "positiveCondition": "true",
                                "negativeCondition": "false"
                            }
                        },
                        "false"
                    ]
                }
            },
            "advisorStatus": {
                "type": "accountAttribute",
                "attributes": {
                    "sourceName": "Advisors - SIS Feed",
                    "attributeName": "STATUS"
                }
            },
            "value": "#if($userType == 'Employee' && $isEndDateWithinPlus21Days == 'true' && ($empStat1_T == 'T' || $empStat1_T == 'None'))true#elseif($userType == 'Contractor' && $isEndDateWithinPlus21Days == 'true')true#elseif($userType == 'Advisor' && $advisorStatus == 'Inactive')true#elseif($userType == 'Advisor' && $advisorStatus == 'Active')false#{else}false#end"
        },
        "internal": false
    }
}